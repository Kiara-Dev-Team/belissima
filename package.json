{
  "name": "belissima",
  "version": "1.0.0",
  "description": "Beautiful CEO BI Dashboard with KPI visualizations",
  "main": "index.html",
  "scripts": {
    "dev": "npx http-server -p 8080 -o",
    "start": "npx http-server -p 8080",
    "generate:comparison-grid": "node create-comparison-grid-svg.js",
    "generate:screenshots": "node capture-screenshots-svg.js",
