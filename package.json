  "author": "",
  "license": "MIT",
  "dependencies": {
    "chart.js": "^4.4.1",
    "d3": "^7.9.0",
    "plotly.js-dist-min": "^3.3.1"
  },
  "devDependencies": {
    "http-server": "^14.1.1"
  }
}
